{
    "version": 2,
    "functions": {
      "api/**/*.js": {
        "runtime": "nodejs18.x"  
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/server.js"  
      }
    ],
    "builds": [
      {
        "src": "dist/server.js",
        "use": "@vercel/node"
      }
    ]
  }
  